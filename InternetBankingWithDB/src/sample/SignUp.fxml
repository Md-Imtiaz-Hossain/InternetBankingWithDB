            <TextField fx:id="accountNumber" blendMode="MULTIPLY" layoutX="28.0" layoutY="109.0" prefHeight="27.0" prefWidth="272.0">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <TextField fx:id="phone" blendMode="MULTIPLY" layoutX="29.0" layoutY="205.0" prefHeight="27.0" prefWidth="272.0">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <Label fx:id="warning" layoutX="16.0" layoutY="14.0" prefHeight="27.0" prefWidth="259.0" textFill="RED">
